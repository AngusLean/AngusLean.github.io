{
  "envId": "mime-blog-0giyqb4nc29f09d3",
  "version": "2.0",
  "framework": {
    "name": "gh-actions-test",
    "plugins": {
       "client": {
        "use": "@cloudbase/framework-plugin-website",
        "inputs": {
          "installCommand": "npm install --prefer-offline --no-audit --progress=false hexo-cli -g",
          "buildCommand": "npm install && hexo g",
          "outputPath": "public",
          "cloudPath": "/",
          "ignore": [".git", ".github", "node_modules", "cloudbaserc.js"]
        }
      }
    }
  }
}